										<input type="text" class="form-control" placeholder="designation *" name="Designation" required="" data-validation-required-message="Please enter a designation.">
										<p class="help-block text-danger"></p>
									</div>
								</div>
								<div class="col-md-6">
									<div class="form-group">
										<input type="text" class="form-control" placeholder="Type *" name="type" required="" data-validation-required-message="Please enter a type.">
										<p class="help-block text-danger"></p>
									</div>
								</div>
                                <div class="col-md-6">
									<div class="form-group">
										<input type="text" class="form-control" placeholder="PrixUNIT *" name="prix" required="" data-validation-required-message="Please enter a prix.">
										<p class="help-block text-danger"></p>
									</div>
								</div>
                                <div class="col-md-6">
									<div class="form-group">
